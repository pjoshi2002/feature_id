github.com/pjoshi2002/feature_id.git

import pandas as pd
import numpy as np
import matplotlib.pyplot as plt
%matplotlib inline

fetre_val=pd.read_csv('D:/downloads/test_csv.csv')
fetre_val.size
fetre_id=fetre_val.drop('time',axis=1)
fetre_id.plot()
fetre_id_mean=fetre_id.rolling(window=20).mean()
fetre_id_mean.plot()
feature=pd.concat([fetre_id,fetre_id.shift(1)],axis=1)
feature.head()
feature.columns=['Actual_id','Forecast_id']
from sklearn.metrics import mean_squared_error
feature_test=feature [1:]
feature_test.to_csv('forecast_data.csv',index='false')
feature_error=mean_squared_error(feature_test.Actual_id, feature_test.Forecast_id)
feature_error
np.sqrt(feature_error)


